/* pages/services/services.wxss */
page {
  background-color: #f5f5f5;
}

.service { 
  background-color: #f5f5f5;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100vw;
  /* height: 90vh; */
} 

.scrollView {
  background-color: #f5f5f5;
  overflow: hidden;
  position: relative; 
  box-sizing: border-box;
  font-family: PingFang-SC-Medium; 
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 0rpx;
}

.parkingList {
  width: 100%;
  /* background: chartreuse; */
} 
.item{
  display: flex;
  position: relative;
  margin: 15px 15px 0 15px;
  justify-content: space-between;
  align-items: center;
  height: 120px;
  background-color: #ffffff;
  border-radius: 4px;
  box-shadow:0px 2px 4px 0px rgba(0,0,0,0.1);
} 

.bottomView{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70px;
  height: 70px;
  border-radius: 35px;
  color: white;
  font-size: 14px;
  right: 15px;
  position: fixed;
  bottom: 40rpx;
}

.money{
  height:33px;
  font-size:28px;
  font-family:HelveticaNeue;
  color:rgba(51,51,51,1);
  line-height:34px;
  text-align: right;
} 

.moneyleft {
  font-size: 14px;
}

.payBtn {
  width:100px;
  height:32px;
  border-radius:16px;
  font-size:15px;
  font-family:PingFang-SC;
  color:rgba(255,255,255,1);
  line-height:32px;
  margin-top: 10px;
} 

.left{
  position: relative;
  float: left;
  margin-left: 15px;
}
.right{
  position: relative;
  float: right;
  margin-right: 15px;
} 
.parkName {
  height:24px;
  font-size:17px;
  font-family:PingFang-SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  line-height:24px;
  margin-bottom: 5px;
} 
.detail {
  height:17px;
  font-size:12px;
  font-family:PingFang-SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:17px;
} 
.footer{
  width: 750rpx;
  position: fixed;
  bottom: 0;
  background: #ffffff;
  margin: 0 auto;

}
.footer-left, .footer-middle, .footer-right{
  display: inline-block;
  margin:10rpx 0 0 147rpx;
  text-align: center;
}

.footer-image{
  width: 54rpx;
  height: 54rpx;
}
.footer-text{
 font-size:20rpx;
 font-family:PingFang-SC-Regular;
}







